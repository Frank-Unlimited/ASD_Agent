# Qwen API 配置（用于对话、推荐、评估）
VITE_QWEN_API_KEY=your-qwen-api-key
VITE_QWEN_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
VITE_QWEN_MODEL=qwen-plus    # qwen-flash / qwen-plus

# DashScope 多模态服务配置（用于图片/视频分析）
VITE_DASHSCOPE_API_KEY=your-dashscope-api-key
VITE_DASHSCOPE_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
VITE_DASHSCOPE_MODEL=qwen3.5-plus

# 阿里云语音服务配置
VITE_ALIYUN_NLS_APPKEY=your-aliyun-nls-appkey
VITE_ALIYUN_NLS_TOKEN=your-aliyun-nls-token

# Qwen-Omni-Realtime 视频通话配置（通过本地代理）
VITE_QWEN_REALTIME_URL=ws://localhost:8765

# 文件大小限制（字节）
VITE_MAX_IMAGE_SIZE=5242880
VITE_MAX_VIDEO_SIZE=52428800

# 文生图模型（默认 wanx2.1-t2i-turbo）
VITE_IMAGE_GEN_MODEL=wanx2.1-t2i-turbo

# 博查AI搜索 API 配置（用于联网搜索）
# 获取方式：https://open.bocha.cn
# 国内可访问，专为AI优化
VITE_BOCHA_API_KEY=your-bocha-api-key
